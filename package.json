{
  "name": "local-duress-management-system-server",
  "version": "1.0.5",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "concurrently \"yarn:start:dev\" \"yarn:gen-typings\"",
    "gen-typings": "ts-node ./src/generate-typings",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "yarn run ts-node typeorm/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "3.213.0",
    "@aws-sdk/client-sns": "3.213.0",
    "@nestjs/apollo": "10.1.6",
    "@nestjs/common": "8.4.7",
    "@nestjs/core": "8.4.7",
    "@nestjs/graphql": "10.1.6",
    "@nestjs/graphql-workspace": "nestjs/graphql",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "8.2.2",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/typeorm": "8.1.4",
    "@urql/core": "2.6.1",
    "apollo-server-core": "3.11.1",
    "apollo-server-express": "3.11.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "concurrently": "7.5.0",
    "cross-fetch": "3.1.5",
    "dotenv": "16.0.3",
    "express": "4.18.1",
    "flatted": "^3.2.7",
    "form-data": "4.0.0",
    "graphql": "16.6.0",
    "graphql-iso-date": "3.6.1",
    "graphql-subscriptions": "2.0.0",
    "libphonenumber-js": "1.10.14",
    "luxon": "2.5.0",
    "passport": "0.5.3",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pdf-lib": "1.17.1",
    "pdfmake": "0.2.6",
    "pg": "8.8.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "ts-morph": "16.0.0",
    "typeorm": "0.3.9",
    "typeorm-naming-strategies": "4.1.0",
    "uuid": "8.3.2",
    "webpack": "5.73.0"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.8",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.7",
    "@types/express": "4.17.14",
    "@types/jest": "27.5.0",
    "@types/node": "*",
    "@types/passport-jwt": "3.0.7",
    "@types/passport-local": "1.0.34",
    "@types/pdfmake": "0.1.21",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.0.3",
    "jsonwebtoken": "8.5.1",
    "prettier": "2.7.1",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
